scope   
    Dim a As Integer
    DIM vysl AS INTEGER

    PrinT !"Zadejte a vetsi nez 42";
    InpuT A
    IF a <= 42 then
        print !"To je drzost, rek jsem 42. Koncim, uz mne to nebavi."
    else
        Vysl = 1
        a = (A - 1) * 15 \ 15 + vysl
        vysl = a + vysl + vysl
        print vysl; !"\n"
    end if
end scope
